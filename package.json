{
  "name": "markdown-vue",
  "displayName": "Markdown Vue",
  "description": "Markdown fenced code block syntax highlighting for Vue",
  "version": "0.0.1",
  "publisher": "donaldshen",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "vue",
    "Markdown"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "markdown.vue.codeblock",
        "path": "./syntaxes/codeblock.json",
        "injectTo": [
          "text.html.markdown"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.vue": "vue"
        }
      }
    ]
  },
  "extensionDependencies": [
    "octref.vetur"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/donaldshen/markdown-vue.git"
  }
}
